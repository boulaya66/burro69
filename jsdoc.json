{
    "source": {
        "include": [
            "src/lib/helpers/src",
            "src/lib/logger/src",
            "src/lib/sadex/src",
            "src/app/tcx/src"
        ],
        "includePattern": ".+\\.(m)?js(doc|x)?$",
        "_excludePattern": "(node_modules/|doc|tests|data)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/typedef-import",
        "node_modules/better-docs/category",
        "node_modules/better-docs/component"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "search": true,
        "default": {
            "staticFiles": {
                "include": []
            },
            "useLongnameInNav": true
        },
        "systemName": "tcx",
        "copyright": "tcx documentation - Philippe Gensane - 2020",
        "includeDate": true,
        "navType": "vertical",
        "theme": "slate",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": true,
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "dateFormat": "dddd Do MMMM YYYY, HH:mm:ss",
        "syntaxTheme": "dark",
        "sort": true,
        "better-docs": {
            "name": "tcx",
            "logo": "",
            "title": "tcx",
            "css": "",
            "hideGenerator": false,
            "navLinks": [
                {
                    "label": "Github",
                    "href": "https://github.com/boulaya66/burro69"
                }
            ]
        }
    },
    "opts": {
        "encoding": "utf8",
        "verbose": true,
        "access": "all",
        "destination": "doc",
        "recurse": true,
        "template-docstrap": "node_modules/ink-docstrap/template",
        "template-docdash": "node_modules/docdash",
        "template-better": "node_modules/better-docs",
        "template": "node_modules/ink-docstrap/template"
    },
    "docdash": {
        "static": true,
        "sort": true,
        "search": true,
        "collapse": true,
        "wrap": true,
        "typedefs": true,
        "removeQuotes": "none",
        "sectionOrder": [
            "Modules",
            "Classes",
            "Externals",
            "Events",
            "Namespaces",
            "Mixins",
            "Tutorials",
            "Interfaces"
        ],
        "meta": {
            "title": "tcx"
        },
        "private": true
    }
}
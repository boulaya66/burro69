{
  "name": "@burro69/sadex",
  "version": "0.1.0",
  "description": "sadejs extensions",
  "author": "Philippe Gensane",
  "license": "MIT",
  "keywords": [
    "sade",
    "cli"
  ],
  "main": "./dist/sadex.js",
  "module": "./dist/sadex.mjs",
  "esmodule": "./dist/sadex.mjs",
  "source": "./src/index.mjs",
  "exports": {
    "import": "./dist/sadex.mjs",
    "require": "./dist/sadex.js"
  },
  "scripts": {
    "test": "node test/test.mjs",
    "lint": "eslint src/**/*.mjs",
    "doc": "jsdoc -c jsdoc.json",
    "bundle": "jsb b --config --cfgfile jsb.json",
    "build": "microbundle -f cjs,modern --target node --no-sourcemap --raw --no-compress"
  },
  "files": [
    "src/",
    "dist/",
    "test/",
    "jsdoc.json"
  ],
  "dependencies": {
    "@burro69/helpers": "0.1.0",
    "@burro69/logger": "0.1.0",
    "ansi-to-html": "^0.6.14",
    "sade": "^1.7.4"
  },
  "devDependencies": {},
  "peerDependencies": {}
}
